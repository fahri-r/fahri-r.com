{
	"name": "fahri-r-com",
	"type": "module",
	"version": "0.0.1",
	"scripts": {
		"dev": "astro dev",
		"build": "astro build",
		"preview": "astro preview",
		"astro": "astro",
		"format": "prettier --write .",
		"sync": "rimraf src/pages/posts/_ && bunx tsx src/notion.ts",
		"sync:published": "rimraf src/pages/posts/_ && bunx tsx src/notion.ts --published",
		"generate": "rimraf dist/*_ && ([ -d 'dist' ] || mkdir dist) && ([ -d 'dist/images' ] || mkdir dist/images) && ([ -d 'src/pages/posts' ] || mkdir src/pages/posts) && ([ -d 'src/assets' ] || mkdir src/assets) && rimraf src/pages/posts/_ && bunx tsx src/notion.ts --published && astro build"
	},
	"dependencies": {
		"@astrojs/mdx": "^4.2.3",
		"@astrojs/sitemap": "^3.3.0",
		"@astrojs/svelte": "^7.0.7",
		"@astrojs/vercel": "^8.1.3",
		"@lucide/svelte": "^0.483.0",
		"@notionhq/client": "^2.3.0",
		"@tailwindcss/vite": "^4.0.15",
		"@types/node": "^22.14.0",
		"astro": "^5.5.4",
		"clsx": "^2.1.1",
		"dotenv": "^16.4.7",
		"mode-watcher": "^0.5.1",
		"moment": "^2.30.1",
		"notion-to-md": "^4.0.0-alpha.4",
		"svelte": "^5.25.2",
		"tailwind-merge": "^3.0.2",
		"tailwind-variants": "^1.0.0",
		"tailwindcss": "^4.0.15",
		"tailwindcss-animate": "^1.0.7",
		"typescript": "^5.8.2"
	},
	"devDependencies": {
		"bits-ui": "^1.3.16",
		"prettier": "^3.5.3",
		"prettier-plugin-astro": "^0.14.1",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11"
	}
}

---
import type {
	Heading1,
	Heading2,
	Heading3,
	SyncedBlock
} from '~/interfaces/notion/block.interface';
import NotionBlock from './notion-block.astro';

interface Props {
	block: SyncedBlock;
	headings: (Heading1 | Heading2 | Heading3)[];
}

const { block, headings }: Props = Astro.props;
---

<NotionBlock blocks={block.children!} {headings} />
